{{ form_start(form) }}

<div class="col-md-12">


    <div class="my-custom-class-for-errors">
        {{ form_errors(form) }}
    </div>

    <div class="row form-special">
            {{ form_row(form.nome,  {'attr': {'class': 'form-control'}}) }}
       

            {{ form_row(form.preco,  {'attr': {'class': 'form-control'}}) }}
    </div>


    <div class="row form-special">
            {{ form_row(form.quantidade,  {'attr': {'class': 'form-control'}}) }}

            {{ form_row(form.descricao,  {'attr': {'class': 'form-control'}}) }}
    </div>


    <div class="row form-special">
            {{ form_row(form.validade,  {'attr': {'class': 'form-control'}}) }}


    </div>


</div>


<button class="btn btn-success">{{ button_label|default('Salvar') }}</button>
{{ form_end(form) }}
